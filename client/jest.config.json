{
	"testEnvironment": "jsdom",
	"transform": {
		"^.+\\.(ts|js|tsx|jsx)$": "@swc/jest"
	},
	"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
	"moduleFileExtensions": [ "ts", "tsx", "js", "jsx", "json", "node" ],
	"moduleNameMapper": {
		"^.+\\.(css|less)$": "<rootDir>/jest/css-stub.js"
	},
	"globals": {
		"NODE_ENV": "test"
	}
}
